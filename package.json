{
  "name": "bun-fastify",
  "version": "1.0.0",
  "description": "bun fastify",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "dev": "bun --env-file=.env.dev --watch src/index.ts",
    "dev2": "bun --env-file=.env.prod src/index.ts",
    "debug": "bun --env-file=.env.dev --inspect-brk src/index.ts",
    "serve": "bun --env-file=.env.prod dist/index.js",
    "build": "bun --env-file=.env.prod build",
    "c-entity": "cross-env NODE_ENV=prod ts-node ./generate/entity.ts && eslint ./src/entity/*.ts --fix --quiet",
    "c-router": "cross-env NODE_ENV=prod ts-node ./generate/router.ts && eslint ./src/router/modules/*.ts --fix --quiet",
    "create": "bun run c-entity && bun run c-router",
    "prisma:pull": "dotenv -e .env.dev -- bun prisma db pull && bun prisma generate",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --fix",
    "czc": "git add . && bun exec cz",
    "preinstall": "npx only-allow bun",
    "prepare": "husky install && prisma generate",
    "up": "taze",
    "up:max": "taze major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imbacc/fast-fastify.git"
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^8.3.0",
    "@fastify/schedule": "^4.1.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^1.10.2",
    "@prisma/client": "^5.15.0",
    "ali-oss": "^6.20.0",
    "fastify": "4.24.3",
    "fastify-no-icon": "^6.0.0",
    "fluent-json-schema": "^4.2.1",
    "imba-md5": "^1.0.0",
    "mysql": "^2.18.1",
    "prisma": "^5.15.0",
    "redis": "^4.6.14",
    "toad-scheduler": "^3.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@eslint/js": "^9.5.0",
    "@fastify/type-provider-json-schema-to-ts": "^2.2.2",
    "@types/bun": "latest",
    "@types/module-alias": "^2.0.4",
    "@types/mysql": "^2.15.26",
    "@types/node": "^20.14.2",
    "commitizen": "^4.3.0",
    "commitlint": "^18.6.1",
    "cz-customizable": "^7.0.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "globals": "^15.4.0",
    "husky": "^8.0.3",
    "imba-config": "^2.0.2",
    "lint-staged": "^15.2.6",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.3",
    "prisma-json-schema-generator": "^5.1.4",
    "taze": "^0.13.8",
    "ts-dotenv": "^0.9.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.13.0",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "_moduleAliases": {
    "@": "src"
  }
}