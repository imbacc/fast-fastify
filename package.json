{
  "name": "fast-fastify",
  "version": "1.0.0",
  "description": "快速构建fastify",
  "main": "src/index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev && nodemon src/index.ts",
    "debug": "cross-env NODE_ENV=dev && nodemon --inspect-brk src/index.ts",
    "serve": "cross-env NODE_ENV=prod && node",
    "build": "cross-env NODE_ENV=prod && tsc -p tsconfig.json",
    "mysqlts": "pnpm dlx mysqltots -u root -p root -h localhost -d test",
    "c-entity": "ts-node ./generate/entity.ts && eslint ./src/entity/*.ts --fix --quiet",
    "c-router": "ts-node ./generate/router.ts && eslint ./src/router/modules/*.ts --fix --quiet",
    "create": "pnpm run c-entity && pnpm run c-router",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --fix",
    "czc": "git add . && pnpm exec cz",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install && prisma generate",
    "up": "taze",
    "up:max": "taze major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imbacc/fast-fastify.git"
  },
  "author": "imbacc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imbacc/fast-fastify/issues"
  },
  "homepage": "https://github.com/imbacc/fast-fastify#readme",
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^8.0.0",
    "@fastify/schedule": "^4.1.1",
    "@fastify/swagger": "^8.13.0",
    "@fastify/swagger-ui": "^1.10.2",
    "@prisma/client": "^5.7.1",
    "ali-oss": "^6.19.0",
    "fastify": "4.24.3",
    "fastify-no-icon": "^6.0.0",
    "fluent-json-schema": "^4.2.1",
    "imba-md5": "^1.0.0",
    "mysql": "^2.18.1",
    "pino-pretty": "^10.3.1",
    "prisma": "^5.7.1",
    "redis": "^4.6.12",
    "toad-scheduler": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@commitlint/config-conventional": "^18.4.3",
    "@fastify/type-provider-json-schema-to-ts": "^2.2.2",
    "@types/module-alias": "^2.0.4",
    "@types/mysql": "^2.15.25",
    "@types/node": "^20.10.6",
    "commitizen": "^4.3.0",
    "commitlint": "^18.4.3",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "imba-config": "^1.0.8",
    "lint-staged": "^15.2.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.0.2",
    "prisma-json-schema-generator": "^5.1.1",
    "taze": "^0.13.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "_moduleAliases": {
    "@": "src"
  }
}
