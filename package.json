{
  "name": "fast-fastify",
  "version": "1.0.0",
  "description": "快速构建fastify",
  "main": "src/index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev && nodemon src/index.ts",
    "debug": "cross-env NODE_ENV=dev && nodemon --inspect-brk src/index.ts",
    "serve": "cross-env NODE_ENV=prod && node",
    "build": "cross-env NODE_ENV=prod && tsc -p tsconfig.json",
    "mysqlts": "pnpm dlx mysqltots -u root -p root -h localhost -d test",
    "c-entity": "ts-node ./generate/entity.ts && eslint ./src/entity/*.ts --fix",
    "c-router": "ts-node ./generate/router.ts && eslint ./src/router/modules/*.ts --fix",
    "create": "pnpm run c-entity && pnpm run c-router",
    "prisma:studio": "pnpm exec prisma studio",
    "prisma:create": "pnpm exec prisma generate",
    "prisma:validate": "pnpm exec prisma validate",
    "prisma:format": "pnpm exec prisma format",
    "prisma:cmysql": "pnpm exec prisma generate",
    "prisma:migrate": "pnpm exec prisma migrate dev --name init",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "czc": "git add . && pnpm exec cz",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "up:list": "ncu",
    "up:change": "ncu -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imbacc/fast-fastify.git"
  },
  "author": "imbacc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imbacc/fast-fastify/issues"
  },
  "homepage": "https://github.com/imbacc/fast-fastify#readme",
  "dependencies": {
    "@fastify/cors": "^8.4.1",
    "@fastify/jwt": "^7.2.2",
    "@fastify/multipart": "^8.0.0",
    "@fastify/schedule": "^4.1.1",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^1.10.1",
    "@prisma/client": "^5.5.2",
    "ali-oss": "^6.18.1",
    "fastify": "4.24.3",
    "fastify-no-icon": "^6.0.0",
    "fluent-json-schema": "^4.1.2",
    "imba-md5": "^1.0.0",
    "mysql": "^2.18.1",
    "pino-pretty": "^10.2.3",
    "prisma": "^5.5.2",
    "redis": "^4.6.10",
    "toad-scheduler": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@commitlint/config-conventional": "^18.1.0",
    "@fastify/type-provider-json-schema-to-ts": "^2.2.2",
    "@types/module-alias": "^2.0.3",
    "@types/mysql": "^2.15.23",
    "@types/node": "^20.8.10",
    "commitizen": "^4.3.0",
    "commitlint": "^18.2.0",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "imba-config": "^1.0.8",
    "lint-staged": "^15.0.2",
    "module-alias": "^2.2.3",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "_moduleAliases": {
    "@": "src"
  }
}
