{
  "name": "fast-fastify",
  "version": "1.0.0",
  "description": "快速构建fastify",
  "main": "src/index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev && nodemon src/index.ts",
    "debug": "cross-env NODE_ENV=dev && nodemon --inspect-brk src/index.ts",
    "serve": "cross-env NODE_ENV=prod && node",
    "build": "cross-env NODE_ENV=prod && tsc -p tsconfig.json",
    "mysqlts": "pnpm dlx mysqltots -u root -p root -h localhost -d test",
    "build-entity": "ts-node ./generate/entity.ts && eslint ./src/entity/*.ts --fix",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "czc": "git add . && pnpm exec cz",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imbacc/fast-fastify.git"
  },
  "author": "imbacc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imbacc/fast-fastify/issues"
  },
  "homepage": "https://github.com/imbacc/fast-fastify#readme",
  "dependencies": {
    "@fastify/jwt": "^7.0.0",
    "@fastify/swagger": "^8.5.1",
    "@fastify/swagger-ui": "^1.8.1",
    "fastify": "4.17.0",
    "fastify-no-icon": "^6.0.0",
    "fluent-json-schema": "^4.1.0",
    "imba-md5": "^1.0.0",
    "mysql": "^2.18.1",
    "pino-pretty": "^10.0.0",
    "redis": "^4.6.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@fastify/cors": "^8.3.0",
    "@fastify/type-provider-json-schema-to-ts": "^2.2.2",
    "@types/module-alias": "^2.0.1",
    "@types/mysql": "^2.15.21",
    "@types/node": "^20.2.5",
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.5",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.42.0",
    "husky": "^8.0.3",
    "imba-config": "^1.0.3",
    "lint-staged": "^13.2.2",
    "module-alias": "^2.2.3",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "webpack": "^5.85.1",
    "webpack-cli": "^5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "_moduleAliases": {
    "@": "src"
  }
}
