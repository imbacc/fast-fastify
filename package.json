{
  "name": "fast-fastify",
  "version": "1.0.0",
  "description": "快速构建fastify",
  "main": "src/index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev && nodemon --config nodemon.json --exec pnpm exec ts-node src/index.ts",
    "debug": "cross-env NODE_ENV=dev && nodemon --config nodemon.json --inspect-brk src/index.ts",
    "serve": "cross-env NODE_ENV=prod && node",
    "build": "cross-env NODE_ENV=prod && tsc -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imbacc/fast-fastify.git"
  },
  "author": "imbacc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imbacc/fast-fastify/issues"
  },
  "homepage": "https://github.com/imbacc/fast-fastify#readme",
  "dependencies": {
    "fastify": "3.11.0",
    "fastify-cors": "^5.2.0",
    "fastify-jwt": "^1.2.1",
    "fastify-no-icon": "^3.0.0",
    "fastify-swagger": "^4.0.1",
    "fluent-json-schema": "^3.0.1",
    "mysql": "^2.17.1",
    "redis": "^3.1.1"
  },
  "devDependencies": {
    "@types/module-alias": "^2.0.1",
    "@types/mysql": "^2.15.21",
    "@types/node": "^17.0.18",
    "@types/redis": "^4.0.11",
    "cross-env": "^7.0.3",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.7",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
